{
  "name": "Teddy Extension",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Adult content filter",
  "permissions": [
    "storage",
    "declarativeContent",
    "activeTab",
    "tabs"
  ],
  "web_accessible_resources": [
    "assets/modelJS/*"
  ],
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "assets/default-icon/teddy-bear16.png",
      "32": "assets/default-icon/teddy-bear32.png",
      "48": "assets/default-icon/teddy-bear48.png",
      "128": "assets/default-icon/teddy-bear128.png"
    }
  },
  "icons": {
    "16": "assets/default-icon/teddy-bear16.png",
    "32": "assets/default-icon/teddy-bear32.png",
    "48": "assets/default-icon/teddy-bear48.png",
    "128": "assets/default-icon/teddy-bear128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "run_at": "document_end",
      "js": [
        "content-script.js",
        "runtime.js",
        "vendor.js"
      ]
    }
  ],
  "background": {
    "page": "index.html#/background"
  }
}


